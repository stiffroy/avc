        "jquery": "^3.2",
        "laravel-mix": "^2.1.11",
        "lodash": "^4.17.10",
        "npm": "^6.0.1",
        "popper.js": "^1.14.3",
        "vue": "^2.5.16",
        "vue-devtools": "^3.1.9",
